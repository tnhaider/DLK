  <text>
    <body>
      <div n="1">
        <lg type="poem">
          <head>Rückerinnerung</head>
          <lg rhyme="unknown" type="stanza">
            <l>Bin ein Mädchen gewesen</l>
            <l>Auf heimischer Flur,</l>
            <l>Mein Denken, mein Fühlen,</l>
            <l>Galt dir, o Natur!</l>
          </lg>
          <lg rhyme="unknown" type="stanza">
            <l>Nun hält mich gefangen</l>
            <l>Ein liebender Blick,</l>
            <l>Doch die Ruhe der Heimat</l>
            <l>War auch schon ein Glück!</l>
          </lg>
